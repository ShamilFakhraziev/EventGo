@model AccountCashOut;
@{
    ViewData["Title"] = "Вывод средств";
}
<h2>Вывод средств</h2>
<form method="post" asp-action="CashOut" asp-controller="Account">
    <div asp-validation-summary="ModelOnly"></div>
    <div>
        <label asp-for="CreditCard"></label> <br />
        <input type="number" asp-for="CreditCard" />
        <span asp-validation-for="CreditCard"></span>
    </div>
    <div>
        <label asp-for="CashOutAmount"></label> <br />
        <input type="number" asp-for="CashOutAmount" />
        <span asp-validation-for="CashOutAmount"></span>
    </div>
    
    <div class="mt-1">
        <input type="submit" class="btn btn-success" value="Вывести" />
    </div>
</form>